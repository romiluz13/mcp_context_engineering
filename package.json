{
  "name": "mcp-context-engineering",
  "description": "Persistent memory banks and collaborative intelligence for AI coding assistants",
  "version": "2.3.0",
  "type": "module",
  "main": "dist/index.js",
  "author": "Rom Iluz <rom@iluz.net>",
  "homepage": "https://github.com/romiluz13/mcp_context_engineering",
  "repository": {
    "type": "git",
    "url": "https://github.com/romiluz13/mcp_context_engineering.git"
  },
  "bin": {
    "mcp-context-engineering": "dist/cli.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "context-engineering",
    "memory-bank",
    "ai-assistant",
    "mongodb",
    "vector-search",
    "collaborative-intelligence",
    "cursor",
    "claude",
    "vscode"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "setup-database": "node scripts/simple-setup.js",
    "generate-sample-data": "node scripts/generate-sample-data.js",
    "setup-memory-bank": "node scripts/setup-memory-bank-collections.js",
    "setup-all": "npm run setup-database && npm run generate-sample-data && npm run setup-memory-bank",
    "setup-manual": "npm run setup-all",
    "test": "node scripts/test-setup.js",
    "test-setup": "node scripts/test-setup.js",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^17.1.0",
    "mongodb": "^6.0.0",
    "openai": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "scripts/simple-setup.js",
    "scripts/generate-sample-data.js",
    "scripts/setup-memory-bank-collections.js",
    "examples/mcp-configs/**/*",
    "docs/CORRECT_MCP_WORKFLOW.md",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT"
}
